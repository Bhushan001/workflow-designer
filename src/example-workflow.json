{
  "id": "workflow-1702310400000",
  "name": "Example API Workflow",
  "createdAt": "2024-12-11T10:00:00.000Z",
  "updatedAt": "2024-12-11T10:00:00.000Z",
  "nodes": [
    {
      "id": "trigger-1",
      "type": "TRIGGER",
      "position": { "x": 100, "y": 200 },
      "data": {
        "label": "Manual Trigger",
        "config": {
          "triggerType": "MANUAL"
        }
      }
    },
    {
      "id": "api-1",
      "type": "API",
      "position": { "x": 350, "y": 200 },
      "data": {
        "label": "Fetch User Data",
        "config": {
          "url": "https://api.example.com/users",
          "method": "GET",
          "headers": {
            "Content-Type": "application/json"
          },
          "timeoutMs": 5000
        }
      }
    },
    {
      "id": "condition-1",
      "type": "CONDITION",
      "position": { "x": 600, "y": 200 },
      "data": {
        "label": "Check Response",
        "config": {
          "expression": "snapshot.nodeOutputs['api-1']?.response?.data?.value > 30"
        }
      }
    },
    {
      "id": "code-1",
      "type": "CODE",
      "position": { "x": 850, "y": 150 },
      "data": {
        "label": "Process Success",
        "config": {
          "code": "// Process the successful response\nconst apiData = snapshot.nodeOutputs['api-1'];\nconsole.log('Processing:', apiData);\nreturn { processed: true };",
          "timeoutMs": 1000
        }
      }
    },
    {
      "id": "donothing-1",
      "type": "DO_NOTHING",
      "position": { "x": 850, "y": 250 },
      "data": {
        "label": "Skip Processing",
        "config": {
          "note": "Condition not met, skip processing"
        }
      }
    }
  ],
  "edges": [
    {
      "id": "edge-1",
      "source": "trigger-1",
      "target": "api-1"
    },
    {
      "id": "edge-2",
      "source": "api-1",
      "target": "condition-1"
    },
    {
      "id": "edge-3",
      "source": "condition-1",
      "target": "code-1",
      "sourceHandle": "true",
      "label": "true"
    },
    {
      "id": "edge-4",
      "source": "condition-1",
      "target": "donothing-1",
      "sourceHandle": "false",
      "label": "false"
    }
  ]
}
